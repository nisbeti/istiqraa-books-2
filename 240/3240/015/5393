<p><span class="anchor" id="p3"></span><span class="c5">أبي قبيل المغافري:</span> ج ٦/ ٢٤٢. ج ٩/ ١٩٢.<a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p4"></span>ج ١٠/ ٢٩، ٤٨، ٥١.<a href="#p4" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p5"></span><span class="c5">قتادة:</span> ج ١/ ٢٠، ٢١، ٢٦، ٣١، ٤١، ٤٣، ٥٥، ٦٣، ٧١، ٧٨، ٧٩، ٨١، ٨٨، ٩١، ٩٦، ١٠٥، ١١٠، ١١٥، ١١٦، ١٤٢، ١٤٤، ١٤٥، ١٤٨، ١٥٠، ١٥٨، ١٥٩، ١٧٠، ١٧٩، ١٨٩، ٢٠٠، ٢٠١، ٢٠٢، ٢٠٣، ٢٠٤، ٢١٢، ٢١٦، ٢١٧، ٢٢٣، ٢٢٦، ٢٢٨، ٢٢٩، ٢٣١، ٢٣٢، ٢٣٣، ٢٣٤، ٢٣٥، ٢٣٦، ٢٤٠، ٢٤١، ٢٤٤، ٢٤٦، ٢٦٥، ٢٦٩، ٢٧٢، ٢٧٣، ٢٨٤، ٢٨٧، ٢٨٨، ٢٨٩، ٢٩٠، ٢٩٤، ٣٠٩، ٣١١، ٣١٣، ٣٢١، ٣٢٤، ٣٢٥، ٣٢٦، ٣٢٧، ٣٣٩.<a href="#p5" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p6"></span>ج ٢/ ٤، ١٠، ١٢، ١٧، ٢٨، ٤٣، ٤٤، ٤٨، ٥٠، ٥٨، ٦٦، ٦٧، ٦٨، ٧٨، ٨٦، ٨٧، ٨٨، ٩٩، ١٠٥، ١٠٦، ١١١، ١١٢، ١١٩، ١٢١، ١٢٢، ١٢٤، ١٢٨، ١٢٩، ١٤٠، ١٤١، ١٤٣، ١٥٩، ٢٢١، ٢٧٧، ٣٠٧، ٣٢١، ٣٢٤.<a href="#p6" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p7"></span>ج ٣/ ٦، ٢٨، ٥٢، ٦٥، ١٠٩، ١١٥، ١١٧، ١١٨، ١١٩، ١٢٤، ١٢٦، ١٣٣، ٢٠٣، ٢٠٨، ٢٠٩، ٢٤١، ٢٤٢، ٢٤٧، ٢٥٢، ٢٦١، ٢٧٥، ٢٨٤، ٢٩٣، ٢٩٨، ٣٠١. ج ٤/ ٩، ٢٤، ٢٧، ٤٦، ٧١، ٩٢، ٩٣، ١٠٩، ١٢٩، ١٤٤، ١٤٥، ١٦٤، ١٧١، ١٧٢، ١٧٩، ١٨٤، ٢١٣، ٢٦٢، ٣٣٩، ٣٤٩، ٣٥٧، ٣٦٤، ٣٦٥.<a href="#p7" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p8"></span>ج ٥/ ٢، ٢٠، ٤٦، ٤٧، ١٠٩، ١١٧، ١٢٠، ١٢٣، ١٢٦، ١٢٧، ١٢٩، ١٣٢، ١٤٠، ١٤١، ١٧١، ١٧٦، ١٧٨، ٢٠٣، ٢٠٤، ٢١٦، ٢٣٨، ٢٣٩، ٢٥٩، ٢٦٤، ٢٧٣، ٢٨٣، ٢٩٢، ٢٩٥، ٢٩٨، ٢٩٩، ٣٠٠، ٣٠٢، ٣٠٦، ٣٠٨، ٣٢٢، ٣٢٨، ٣٤٢.<a href="#p8" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p9"></span>ج ٦/ ٢، ٥، ٢٠، ٢٢، ٢٦، ٣٥، ٣٦، ٣٧، ٥٢، ٦٠، ٧٤، ٨٨، ٩٤، ١٥١، ١٦٩، ١٩٩، ٢١٣، ٢١٦، ٢١٧، ٢٣٩، ٣١٢. ج ٧/ ١٣٩، ١٨٥، ١٩٣، ٢٠٠، ٢٠٢، ٢٢٢، ٢٤٠، ٢٦٦، ٢٧٨، ٢٩٦، ٣٠٢، ٣٠٣، ٣٣٥، ٣٥١.<a href="#p9" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p10"></span>ج ٨/ ١٣٨، ١٣٩. ج ٩/ ٩٢، ٩٤، ١٣٦، ١٥٤، ١٩٠، ١٩٧، ٢٠٠، ٢٤٥، ٢٦٦، ٣٠٦، ٣٤٣. ج ١٠/ ١٠٧، ٢٢٩.<a href="#p10" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
