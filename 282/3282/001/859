<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span>الصافية <span class="c2">(حائط)</span> : ٥٦١، ٥٦٦ الصافية <span class="c2">(موضع)</span> : ٥٦٦ صرار: ٥٨٦، ٥٨٨، ٥٩١،<span class="c5"> ٥٩٣ الصغد:</span> ٤٢٧ الصفراء: ٣٦٣ صفين: ٩٦، ٩٧، ٢٧٨، ٣٢٦، ٣٥٢،<span class="c5"> ٦٩٥ صنعاء:</span> ٢٦٩، ٢٧٣، ٤٩٨، ٤٩٩، ٥٤٥،<span class="c5"> ٦٦٢ الصواري:</span> ١٨٠ الصوارين: ٢١٥،<span class="c5"> ٢١٦ ضرية:</span> ٦٤٥ الطائف: ٨٦، ١٤٠، ١٦٤، ٢٠٨، ٢٥٨، ٣٠٧، ٤٩١، ٤٩٢، ٤٩٣، ٤٩٥، ٥٠٣، ٥٠٥، ٥٠٦، ٥١٥، ٥٤٥، ٦٧٠، ٦٩٦،<span class="c5"> ٧١١ الطف:</span> ١٤٥ الظريبة: ١٧٤ الظهران: ٧٣٠ العاقول: ٣٨٩ العالية: ٥١١، ٥٦٤، ٧٤٩،<span class="c5"> ٧٥٠ العراق:</span> ٥٠، ٥٥، ١٠٢، ٢٩٤، ٣٧١، ٥٠٣، ٥٣٠، ٥٣٧، ٥٧٦، ٥٧٧، ٥٨٤، ٦١٤، ٦٢١ عرفات <span class="c2">(عرفة)</span> : ٢٤٧،<span class="c5"> ٦٦٢ عسفان:</span> ٤٤٢، ٤٤٣،<span class="c5"> ٤٧٤ عسقلان:</span> ١٧٢،<span class="c5"> ١٨٠ العقبة:</span> ٢٨١ العقبة الثانية: ٢٣٢ العقيق: ٤٦٠،<span class="c5"> ٦٥٥ عكاظ:</span> ٢٥٩،<span class="c5"> ٥١٢ العلياء:</span> ٣٩٠ عمان: ١٩٥،<span class="c5"> ٢٠٠ عمواس:</span> ٢٨١، ٢٨٢،<span class="c5"> ٣٨١ عير:</span> ٦٣٠،<span class="c5"> ٦٣٢ العيص:</span> ٣٥٨ عين أبي نيزر: ٥٦٧،<span class="c5"> ٥٦٨ عين التمر:</span> ٦٧ الغابة <span class="c2">(يوم)</span> : ٦٣٠، ٦٣٢، ٧٢٧،<span class="c5"> ٧٢٨ غدير الأشطاط:</span> ٣٣٨، ٣٣٩، ٤٧٤،<span class="c5"> ٤٧٥ غدير خم:</span> ٢٧٤ الغمير: ٤٧٣ الغميم: ٣٤١ غور البقيع <span class="c2">(اقرأ: النقيع)</span> : ٦٤٣ فارس: ١٠٠، ١٩٥، ٢٦٤، ٤٦٠، ٤٩٨، ٦٠٠، ٦٠٩، ٦٢٨، ٦٧١، ٦٨٦، ٦٩٢ فارع <span class="c2">(أطم)</span> : ٧٦٣،<span class="c5"> ٧٦٤ فدك:</span> ٣٠١ الفرات: ٢٨٠،<span class="c5"> ٣٨٩ الفرع:</span> ٣٥٨ الفسطاط: ١٨٠،<span class="c5"> ٤٣٧ فلسطين:</span> ٨٠ القادسية: ١٢٥، ١٧٨، ٢٥٤، ٤٥٩،<span class="c5"> ٤٦٠ قباء:</span> ١٢٢، ١٢٧، ٤٨٩، ٦٣٠، ٧١٧،<span class="c5"> ٧٢٠ قبرس:</span> ٤٨٩، ٤٩٠<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>