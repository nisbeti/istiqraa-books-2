<p><span class="anchor" id="p2"></span><span class="c5">آل أبي بكرة:</span> ٤٧١ آل داود: ١٠٠ آل ساسان: ٢٥٥ آل سعيد بن العاص: ٥٤٢ آل شماس بن لأي: ٣٦ آل عتبة بن ربيعة: ٩٩ آل هاشم: ٢٢٦ الأحابيش: ٢٠٢،<span class="c5"> ٦٣٠ الأحزاب:</span> ١٠١، ٣٥٠،<span class="c5"> ٤٧٧ الأزد:</span> ١٨٩، ٢٦١، ٣٤٥، ٣٨٩،<span class="c5"> ٦٤١ أسلم:</span> ٣٠٢، ٣٤٠، ٣٤١، ٣٩٦، ٣٧٦، ٤٤٣، ٦٦٣،<span class="c5"> ٦٦٤ أشجع:</span> ١١٦ الأشعريون: ٩٩،<span class="c5"> ١٠٠ أصبح:</span> ٢٠٤ الأعاجم: ١٠٥، ٢٢٠، ٢٤٤، ٢٦٩، ٣٨٩، ٤٦٠، ٥٦٨،<span class="c5"> ٧٨٠ الأعجميون:</span> ٥٢٦ الأكاسرة: ٦٢٧،<span class="c5"> ٦٢٨ الأنباط:</span> ٢١٨،<span class="c5"> ٥٢٣ الأنصار:</span> ٣٩، ٤٠، ٨٤، ٨٥، ٩٠، ٩٨، ١٠١، ١١٤، ١٥٢، ١٥٤، ١٦٤، ١٧٩، ٢٠٥، ٢٢٤، ٢٣٢، ٢٣٧، ٢٤٠، ٢٤٩، ٢٥٠، ٢٦٢، ٢٧٤، ٢٧٦، ٢٨١، ٢٩٤، ٢٩٧، ٣٠٣، ٣٢٢، ٣٤٤، ٣٤٥، ٣٥٠، ٣٥٣، ٣٥٤، ٣٥٥، ٣٥٧، ٣٦٠، ٣٦١، ٣٦٤، ٣٦٥، ٤٣٩، ٤٥٧، ٤٦٨، ٤٧٣، ٤٩٧، ٥٤٢، ٦٢٢، ٦٨٨، ٦٨٩، ٧٠٣، ٧١١، ٧١٨، ٧٣٦، ٧٤٠، ٧٤٢، ٧٥٦، ٧٥٧، ٧٦٠،<span class="c5"> ٧٦٢ أهل الجمل:</span> ٤٧٨ أهل الحرمين: ٦٢٠،<span class="c5"> ٦٢١ أهل السواد:</span> ٥٣٠ أهل الصفة: ٦٩، ٧٠، ٧٩، ٨٠، ٨٤، ٨٥،<span class="c5"> ٦٨١ أهل نجد:</span> ٧٥٥ الأوس: ٢٤٩، ٣٠٢، ٣٤٥، ٣٤٩، ٣٥٠، ٣٨٥،<span class="c5"> ٧٥٩ بجيلة:</span> ٤٥٩ البدريون <span class="c2">(أهل بدر)</span> : ١٩٨، ١٩٩، ٢٠٣، ٢١٣، ٣٤٩، ٤٠٤، ٧٢٧ البصريون <span class="c2">(أهل البصرة)</span> : ٢٩٤،<span class="c5"> ٧٥٣ بطارقة الروم:</span> ٧٨٧ بنو أبي الحقيق: ٤٥٧ بنو أسد: ٣١٧، ٥٠٩،<span class="c5"> ٥٤٣ بنو أسد بن عبد العزى:</span> ١٩٧ بنو إسرائيل: ١٦٥،<span class="c5"> ٤٤٣ بنو أمية:</span> ١٠٨، ١٧٦،<span class="c5"> ٥٢٣ بنو أنمار:</span> ٦٦٨ بنو بنت مخزوم: ٢٢٦<a href="#p2" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>