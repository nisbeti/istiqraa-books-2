<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">قينقاع:</span> انظر: بنو قينقاع كلب: انظر: بنو كلب كندة: ٢٠١، ٥٤٢، ٦٩٥ الكوفيون <span class="c2">(أهل الكوفة)</span> : ٣٩٠، ٤٨٠، ٥٣٥، ٦٢٢،<span class="c5"> ٦٩٥ لوذان بن سعد بن جمح:</span> ١٢٦ لؤي: انظر: بنو لؤي المالكية: ٢٩٤ المدنيون <span class="c2">(أهل المدينة)</span> : ٣٥٣، ٤٧٥، ٥٠٣، ٥٦٨، ٥٩٦، ٥٩٧، ٦٠٠، ٦١٩،<span class="c5"> ٦٢٢ مذحج:</span> ١٩٧، ٥٤٢،<span class="c5"> ٥٤٥ مراد:</span> ٢٠٤، ٢٧٩،<span class="c5"> ٥٤٢ مزينة:</span> ٣٠٢، ٣٥٥، ٣٦٩،<span class="c5"> ٣٧٦ مضر بن نزار:</span> ٢٤٩، ٤١٣،<span class="c5"> ٤١٤ معدّ:</span> ٥١٣ المكيون <span class="c2">(أهل مكة)</span> : ٢٦٧، ٢٦٨، ٣٥٧، ٥٩٦، ٥٩٧، ٦٠٠، ٦٠١،<span class="c5"> ٧٦٢ المهاجرون:</span> ٣٩، ٥٤، ٩٦، ١٠١، ١٩٨، ١٩٩، ٢١٣، ٢٣٧، ٢٧٤، ٢٧٦، ٢٩٤، ٢٩٧، ٣٠٣، ٣٤٤، ٣٥٥، ٣٥٧، ٣٦٩، ٤٥٣، ٤٦٢، ٤٧١، ٤٨٧، ٤٩٧، ٦٢٢، ٦٩٢، ٧١٨،<span class="c5"> ٧٣٦ النبط:</span> انظر: الأنباط نزار: ٤٢٧ النصارى: ٢٢٠، ٣٢٢، ٥٢٢، ٥٢٣، ٧٨٠،<span class="c5"> ٧٨١ نصارى الحيرة:</span> ٧٨٠ نصارى نجران: ٥١٩ النمر بن قاسط: ١٦٣،<span class="c5"> ٢٢٧ النوبة:</span> ١٨٠ هوازن: ٢٥٨، ٢٦٧، ٣٧٣، ٣٧٤،<span class="c5"> ٤٦٨ اليمانية:</span> ٢٤٠ اليهود: ١٠١، ٢١٩، ٢٢٠، ٢٣٠، ٤٤٦، ٤٧٩، ٤٩٧، ٥٢٢، ٥٢٣، ٥٥٦، ٥٦١، ٦١٠، ٦٩٠، ٧١٤، ٧٨٠،<span class="c5"> ٧٨١ يهود خيبر:</span> ٥٩١ يهود المدينة: ٧١٤<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>